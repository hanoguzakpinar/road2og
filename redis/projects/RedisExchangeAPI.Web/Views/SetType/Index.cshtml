@model HashSet<string>

@{
    ViewData["Title"] = "Set Type Page";
}

<form method="post" asp-action="Add" enctype="application/x-www-form-urlencoded">
    <input type="text" name="name">
    <input type="submit" value="Kaydet">
</form>

<h1>Names:</h1>
@foreach (string item in Model)
{
    <p>@item
        <a asp-controller="SetType" asp-action="Remove" asp-route-name="@item">Sil</a>
    </p>
}